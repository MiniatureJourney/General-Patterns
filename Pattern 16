public class AlphabetPattern {
    public static void main(String[] args) {
  
int x;
x='A';
        for (int i = 0; i<5; i++) { 
            for (int j = 0; j<=i; j++) { 
                System.out.print((char) (x));
               
                
            }
             x++;
            System.out.println(); 
        }
    }
}
