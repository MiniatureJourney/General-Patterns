public class AlphabetPattern {
    public static void main(String[] args) {
   

        for (int i = 0; i<5; i++) { 
        for (int j = 0; j<i; j++) { 
                System.out.print((char) ('A' + j));
            }
              for (int j =i-1; j>0; j--) { 
                System.out.print((char) (('A'-1) + j));
            }
            System.out.println(); 
        }
    }
}
