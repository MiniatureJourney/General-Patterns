public class pattern1 {
    public static void main(String[] args) {
        int i;
        int j;

        for (i = 5; i >=0; i--) {
            
             for (int k = 4; k >=i; k--) {
                System.out.print(" "); 
            }
for (j = 0; j < 2 * i + 1; j++) {
                System.out.print("*");
            }
       
            
           


            System.out.println();
        }
    }
}
